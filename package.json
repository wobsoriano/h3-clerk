{
  "name": "h3-clerk",
  "version": "0.3.7",
  "packageManager": "pnpm@8.11.0",
  "description": "Unofficial Clerk middleware for H3.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/h3-clerk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/h3-clerk.git"
  },
  "bugs": "https://github.com/wobsoriano/h3-clerk/issues",
  "keywords": [
    "auth",
    "authentication",
    "passwordless",
    "session",
    "jwt",
    "h3"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "h3": "^1.8.0"
  },
  "dependencies": {
    "@clerk/backend": "^0.34.2",
    "@clerk/clerk-sdk-node": "4.12.22"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.3.1",
    "@types/supertest": "^2.0.16",
    "changelogen": "^0.5.5",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.29.0",
    "h3": "^1.9.0",
    "supertest": "^6.3.3",
    "taze": "^0.13.0",
    "tsup": "8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.1",
    "vitest-fetch-mock": "^0.2.2"
  }
}
