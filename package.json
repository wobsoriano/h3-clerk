{
  "name": "h3-clerk",
  "type": "module",
  "version": "0.5.34",
  "packageManager": "pnpm@9.13.2",
  "description": "Unofficial Clerk middleware for H3.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/h3-clerk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/h3-clerk.git"
  },
  "bugs": "https://github.com/wobsoriano/h3-clerk/issues",
  "keywords": [
    "auth",
    "authentication",
    "passwordless",
    "session",
    "jwt",
    "h3"
  ],
  "sideEffects": false,
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepublishOnly": "pnpm build",
    "release": "changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "h3": "^1.8.0"
  },
  "dependencies": {
    "@clerk/backend": "^1.27.0",
    "@clerk/shared": "^3.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@changesets/cli": "^2.27.9",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.24.0",
    "h3": "^1.13.0",
    "supertest": "^7.0.0",
    "tsdown": "^0.9.6",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5",
    "vitest-fetch-mock": "^0.4.2"
  }
}
