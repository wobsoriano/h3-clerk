{
  "name": "h3-clerk",
  "version": "0.5.12",
  "packageManager": "pnpm@9.13.2",
  "description": "Unofficial Clerk middleware for H3.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/h3-clerk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/h3-clerk.git"
  },
  "bugs": "https://github.com/wobsoriano/h3-clerk/issues",
  "keywords": [
    "auth",
    "authentication",
    "passwordless",
    "session",
    "jwt",
    "h3"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "h3": "^1.8.0"
  },
  "dependencies": {
    "@clerk/backend": "^1.17.0",
    "@clerk/shared": "^2.15.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.1",
    "@changesets/cli": "^2.27.9",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.15.0",
    "eslint-plugin-import": "^2.31.0",
    "h3": "^1.13.0",
    "supertest": "^7.0.0",
    "tsup": "8.0.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5",
    "vitest-fetch-mock": "^0.4.2"
  }
}
